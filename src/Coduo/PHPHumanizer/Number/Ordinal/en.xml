<?xml version="1.0" encoding="utf-8" ?>
<ordinal>
	<irregular>
		<number pattern="/^(11|12)$/">
			<prefix></prefix>
			<suffix>th</suffix>
		</number>
		<number pattern="/^{\-}+{\d}+1$/">
			<prefix></prefix>
			<suffix>st</suffix>
		</number>
		<number pattern="/^{\-}+{\d}+2$/">
			<prefix></prefix>
			<suffix>nd</suffix>
		</number>
	</irregular>
	<regular>
		<prefix></prefix>
		<suffix>th</suffix>
	</regular>
</ordinal>